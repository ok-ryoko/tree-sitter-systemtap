{
  "name": "tree-sitter-systemtap",
  "version": "0.1.0",
  "description": "SystemTap grammar for tree-sitter",
  "keywords": [
    "parser",
    "lexer"
  ],
  "bugs": {
    "url": "https://github.com/ok-ryoko/tree-sitter-systemtap/issues"
  },
  "license": "MIT",
  "author": "OK Ryoko",
  "main": "bindings/node",
  "repository": {
    "type": "git",
    "url": "https://github.com/ok-ryoko/tree-sitter-systemtap"
  },
  "scripts": {
    "build": "tree-sitter generate",
    "fmt": "prettier --write .",
    "fmt-check": "prettier --check .",
    "lint": "eslint .",
    "prepare": "husky install",
    "test": "tree-sitter test"
  },
  "dependencies": {
    "nan": "~2.17.0"
  },
  "devDependencies": {
    "eslint": "~8.43.0",
    "eslint-config-prettier": "~8.8.0",
    "husky": "~8.0.3",
    "prettier": "~2.8.8",
    "tree-sitter-cli": "~0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "source.systemtap",
      "file-types": [
        "stp",
        "stpm"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
